<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Bmbp应用</title>
    <link rel="stylesheet" href="/ui/lib/arco/arco.min.css"/>
    <link rel="stylesheet" href="/ui/lib/bmbp/bmbp.css"/>
    <link rel="stylesheet" href="/ui/init/css/init.css"/>

</head>
<body>
<div id="app">
</body>
<script>
    const initValue = {{bmbp}};
    const getAppValue = ()=>{
        if (initValue && initValue.bmbp && initValue.bmbp.app) {
            return initValue.bmbp.app;
        }
        return {};
    }
    const getServerValue = ()=>{
        if (initValue && initValue.bmbp && initValue.bmbp.server) {
            return initValue.bmbp.server;
        }
        return {};
    }
    const getDatasourceValue = ()=>{
        if (initValue && initValue.bmbp && initValue.bmbp.datasource) {
            return initValue.bmbp.datasource;
        }
        return {};
    }
</script>
